{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "min-height: auto"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "create-element-svg": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "apple-touch-icons": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off"
  }
}